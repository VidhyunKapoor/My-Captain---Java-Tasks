import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
		double area,perimeter;
		double r;
		Scanner scan=new Scanner(System.in);
		r=scan.nextDouble();
		area=3.14*r*r;
		perimeter=2*3.14*r;
		System.out.println("Area = "+area);
		System.out.print("Perimeter = "+perimeter);
	}
}
